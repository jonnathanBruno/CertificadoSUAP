<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jonnathanbruno.suapproject">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="SUAP"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.jonnathanbruno.suapproject.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.jonnathanbruno.suapproject.Principal"
            android:label="@string/title_activity_principal"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name="com.example.jonnathanbruno.suapproject.NotasSemestre"
            android:label="@string/title_activity_principal"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name="com.example.jonnathanbruno.suapproject.FaltasAluno"
            android:label="@string/title_activity_principal"
            android:theme="@style/AppTheme">
        </activity>
    </application>

</manifest>